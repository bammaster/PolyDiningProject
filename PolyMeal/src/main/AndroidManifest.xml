<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mustangexchange.polymeal"
    android:versionCode="10"
    android:versionName="2.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="16" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/CalPolyHolo" >
        <activity
            android:name=".PolyDiningActivity"
            android:clearTaskOnLaunch="true"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
                  android:name=".VenueActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".PolyMealActivity"
                  android:label="@string/poly_meal_activity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".CartActivity"
                  android:label="@string/activity_cart"
                  android:screenOrientation="portrait"/>
        <activity android:name=".PlusDollarsActivity"
                  android:label="@string/plus_dollar_activity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".TransactionActivity"
                  android:label="@string/transaction_activity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".CompleteorActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name=".SettingsActivity"
                  android:label="@string/activity_settings"
                  android:screenOrientation="portrait"/>
        <activity android:name=".BaseActivity"/>
        <activity android:name=".BaseFragmentActivity"/>
    </application>



</manifest>